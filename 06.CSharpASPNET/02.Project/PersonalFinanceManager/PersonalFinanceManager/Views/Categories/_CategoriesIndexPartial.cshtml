@model ICollection<PFM.Models.Category>
<table>
    @if (Model.Count != 0)
    {
        foreach (var category in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => category.Name)
                </td>
            </tr>
        }
    }
    else
    {
        <tr>
            <td>No records</td>
        </tr>
    }
</table>